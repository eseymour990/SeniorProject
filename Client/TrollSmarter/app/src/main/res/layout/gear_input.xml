                android:id="@+id/twozero"/>
        </LinearLayout>
        <LinearLayout>

            <TextView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_weight=".25"
                android:text="25 ft:" />

            <EditText
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_weight=".75"
                android:inputType="number"
                android:id="@+id/twofive"/>
        </LinearLayout>
        <LinearLayout>

            <TextView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_weight=".25"
                android:text="30 ft:" />

